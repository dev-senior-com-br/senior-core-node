{
  "name": "@seniorsistemas/senior-core",
  "version": "1.3.0",
  "description": "Senior Core SDK para Node.js",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "author": "Senior ECOAPI Team",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "npm run build",
    "prebuild": "del-cli --force ./dist",
    "build": "tsc",
    "format": "prettier --write \"lib/**/*.ts\" \"lib/**/*.js\"",
    "lint": "eslint ./lib",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run lint",
    "ts-node": "ts-node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dev-senior-com-br/senior-core-node.git"
  },
  "bugs": {
    "url": "https://github.com/dev-senior-com-br/senior-core-node/issues"
  },
  "keywords": [
    "node.js",
    "senior",
    "api",
    "authentication",
    "authorization",
    "tenant",
    "users"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "dotenv": "^8.2.0"
  },
  "homepage": "https://github.com/dev-senior-com-br/senior-core-node#readme",
  "directories": {},
  "devDependencies": {
    "del-cli": "^3.0.1",
    "@types/node": "^14.0.14",
    "tsc": "^1.20150623.0",
    "typescript": "^3.9.5",
    "prettier": "2.0.5",
    "ts-node": "^8.10.2",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "eslint": "^7.3.1"
  }
}
